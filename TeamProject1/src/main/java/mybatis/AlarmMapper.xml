<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mybatis.AlarmMapper">
    <insert id="insertAlarm" parameterType="vo.Alarm">
        INSERT INTO alarm (user_no, type, content)
        VALUES (#{userNo}, #{type}, #{content})
    </insert>
</mapper>
